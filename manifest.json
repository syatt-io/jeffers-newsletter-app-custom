{
  "vendor": "jefferspet",
  "name": "newsletter-app",
  "version": "0.0.4",
  "title": "Newsletter App",
  "description": "Custom Newsletter App",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "store": "0.x"
  },
  "dependencies": {
    "vtex.render-runtime": "0.x",
    "vtex.css-handles": "0.x",
    "vtex.styleguide": "9.x",
    "vtex.disclosure-layout": "1.x",
    "vtex.my-account": "1.x",
    "vtex.my-account-commons": "1.x",
    "vtex.store-graphql": "2.x",
    "vtex.apps-graphql": "2.x"
  },
  "registries": [
    "smartcheckout"
  ],
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "myvtex.com",
        "path": "/_v/graphql/private/v1"
      }
    }
  ],
  "credentialType": "absolute",
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
